            android:id="@+id/action_jobSeekerProfileFragment_to_jobSeekerMediaFragment"
            app:destination="@id/jobSeekerMediaFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left" />
        <action
            android:id="@+id/action_jobSeekerProfileFragment_to_jobSeekerCvFragment"
            app:destination="@id/jobSeekerCvFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/jobSeekerCourseFragment"
        android:name="com.findajob.jobamax.jobseeker.course.JobSeekerCourseFragment"
