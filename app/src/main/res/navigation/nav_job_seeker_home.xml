        android:name="com.findajob.jobamax.jobseeker.profile.JobSeekerProfileFragment"
        android:label="JobSeekerProfileFragment"
        tools:layout="@layout/fragment_job_seeker_profile">
        <action
            android:id="@+id/action_jobSeekerProfileFragment_to_jobSeekerAccountFragment"
            app:destination="@id/jobSeekerAccountFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left" />
        <action
            android:id="@+id/action_jobSeekerProfileFragment_to_jobSearchFragment2"
            app:destination="@id/jobSearchFragment2"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left" />
