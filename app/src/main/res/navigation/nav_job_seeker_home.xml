            app:destination="@id/createEducationFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_jobSeekerCvFragment_to_createExperienceFragment2"
            app:destination="@id/createExperienceFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"  />
    </fragment>
    <fragment
        android:id="@+id/createEducationFragment"
        android:name="com.findajob.jobamax.jobseeker.profile.cv.education.CreateEducationFragment"
        android:label="CreateExperienceFragment"
        tools:layout="@layout/fragment_create_education">

        <argument
            android:name="arg_education"
            android:defaultValue="" />
    </fragment>
    <fragment
        android:id="@+id/createExperienceFragment"
        android:name="com.findajob.jobamax.jobseeker.profile.cv.experience.CreateExperienceFragment"
